{
  "name": "doorman",
  "version": "0.1.1-pre",
  "description": "Multi-platform chatbot framework.",
  "main": "lib/doorman.js",
  "scripts": {
    "docs": "npm run make:docs && http-server docs/",
    "make:api": "npm run parse:api && jsdoc lib/ README.md -d docs/",
    "make:docs": "jsdoc lib README.md -c jsdoc.json -d docs",
    "parse:api": "jsdoc2md --configure jsdoc.json --files lib/** README.md > API.md",
    "test": "NODE_ENV=test mocha tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabricLabs/doorman.git"
  },
  "keywords": [
    "agent",
    "bot",
    "chat",
    "chatbot",
    "community",
    "discord",
    "fabric",
    "maki",
    "matrix",
    "slack"
  ],
  "author": "Fabric Labs <labs@fabric.pub> (https://labs.fabric.pub)",
  "contributors": [
    "Eric Martindale <eric@ericmartindale.com> (https://www.ericmartindale.com)",
    "Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FabricLabs/doorman/issues"
  },
  "homepage": "https://github.com/FabricLabs/doorman#readme",
  "dependencies": {
    "@fabric/core": "FabricLabs/fabric#fabric-core-refactor",
    "@slack/client": "^4.1.0",
    "discord.js": "^11.4.2",
    "erm": "^0.0.1",
    "fast-json-patch": "^2.0.7",
    "json-pointer": "^0.6.0",
    "level": "^4.0.0",
    "marked": "^0.3.19",
    "matrix-js-sdk": "^0.10.1",
    "mkdirpsync": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "http-server": "^0.11.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "FabricLabs/jsdoc",
    "mocha": "^4.0.0",
    "semistandard": "^11.0.0"
  },
  "semistandard": {
    "ignore": [
      "/test/"
    ]
  }
}
